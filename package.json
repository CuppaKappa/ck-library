{
  "name": "ck-library",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "Neoplatonist",
  "license": "MIT",
  "repository": "Neoplatonist/ck-library",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=publish microbundle-crl --no-compress --format modern,cjs",
    "start": "NODE_ENV=development microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-controls": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.2.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-force-resolutions": "^0.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "prettier": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^17.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "storybook-zeplin": "^1.7.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react-hook-form": "^7.25.1"
  }
}
